{
  "name": "paycheck-calculator-web",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular2",
    "webpack2",
    "typescript2"
  ],
  "author": "Tyler Technologies",
  "scripts": {
    "build:debug": "dotnet build -c Debug",
    "build:release": "dotnet build -c Release",
    "build:client": "webpack --config webpack.config.js",
    "build:container:debug": "docker build --build-arg Configuration=Debug . -t paycheck-calculator-web",
    "build:container:release": "docker build --build-arg Configuration=Release . -t paycheck-calculator-web",
    "clean:publish:debug": "npm run rimraf -- bin/Debug/netcoreapp3.1/publish",
    "clean:publish:release": "npm run rimraf -- bin/Release/netcoreapp3.1/publish",
    "clean:publish": "npm run clean:publish:debug && npm run clean:publish:release",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "publish:debug": "dotnet publish -c Debug",
    "publish:release": "dotnet publish -c Release",
    "publish:all:debug": "npm run publish:debug && npm run build:container:debug",
    "publish:all:release": "npm run publish:release && npm run build:container:release",
    "extract": "ngx-translate-extract --input ./Client/app --output ./wwwroot/i18n/template.pot --clean --sort --format pot",
    "echo:creation": "echo \"Generated on 02/18/2019 with Dapper Diablo 1.0.58\"",
    "rimraf": "rimraf",
    "tslint": "tslint -t stylish --project tsconfig.json",
    "webpack": "webpack",
    "protractor": "protractor",
    "dev:watch": "set ASPNETCORE_ENVIRONMENT=Development && dotnet watch run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "lint": "npm run tslint \"Client/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm-run-all -p -r server:prod:ci e2e:debug",
    "e2e:debug": "protractor --elementExplorer",
    "test": "npm run lint && karma start",
    "compodoc": "compodoc -p tsconfig.compodoc.json --theme vagrant",
    "compodoc:watch": "npm run compodoc -- -s -w",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "tsc": "tsc",
    "tsc:noEmit": "tsc --noEmit"
  },
